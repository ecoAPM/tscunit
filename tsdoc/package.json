{
	"name": "tsdoc",
	"version": "1.0.0",
	"description": "Documentation generator for xunit.ts",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">= 14"
	},
	"devDependencies": {
		"@microsoft/tsdoc": "0.14.2",
		"@types/node": "18.16.3",
		"notosans": "5.0.0",
		"ts-mockito": "2.6.1",
		"ts-node": "10.9.1",
		"typescript": "5.0.4",
		"xunit.ts": "file:../core"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "cp -r node_modules/notosans/* ../docs/assets && ts-node tsdoc.ts",
		"test": "tsc && xunit dist/tsdoc/tests"
	}
}
